{
    "name": "shorlyn/repository",
    "description": "A repository package.",
    "keywords": ["laravel", "repository", "repositories", "eloquent", "database"],
    "license" : "MIT",
    "authors" : [
        {
            "name": "shorlyn",
            "email": "shorlyn@126.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.*",
        "illuminate/database": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Shorlyn\\Repositories\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
